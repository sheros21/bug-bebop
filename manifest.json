{
    "manifest_version": 2,
    "name": "Bug Bepop",
    "description": "Exceptional Soundboarding: A Chrome extension that detects errors during code compilation on LeetCode",
    "version": "2.0",
    "permissions": [
      "activeTab",
      "http://*/",
      "https://*/" 
    ],
    "content_scripts": 
    [
      {
        "matches": ["https://leetcode.com/*"],
        "js": ["content-script.js"]
      }
    ],
    "web_accessible_resources": [
      "soundeffect.mp3"
    ]
}
  